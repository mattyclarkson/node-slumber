{
  "name": "slumber",
  "version": "0.10.0",
  "description": "Port of Python's slumber library -- A library that makes consuming a RESTful API easier and more convenient",
  "main": "lib/index.js",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha tests",
    "watch-test": "./node_modules/mocha/bin/mocha -w tests",
    "build": "./node_modules/coffee-script/bin/cake build",
    "watch": "./node_modules/coffee-script/bin/cake watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moul/node-slumber"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "querystring": "^0.2.0",
    "request": "^2.72.0"
  },
  "devDependencies": {
    "body-parser": "^1.15.2",
    "coffee-script": "^1.10.0",
    "express": "^4.14.0",
    "freeport": "^1.0.5",
    "mocha": "^2.5.3",
    "url": "^0.11.0",
    "yamljs": "^0.2.8"
  },
  "optionalDependencies": {
    "yamljs": "0.2"
  },
  "keywords": [
    "slumber",
    "python",
    "api",
    "request",
    "rest",
    "json",
    "yaml",
    "restful"
  ],
  "author": "Manfred Touron <m@42.am>",
  "license": "MIT",
  "readmeFilename": "README.md"
}
